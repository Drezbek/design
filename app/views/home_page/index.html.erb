<section id="contact" class="contacts-section">
  <div class="container content-lg">
    <div class="wow slideInLeft title-v1">
      <h2>Contáctanos</h2>
      <p>Escríbenos, nos comunicaremos contigo a la brevedad</p>
    </div>

    <div class="row contacts-in">
      <div class="col-md-6 md-margin-bottom-40">
        <ul class="list-unstyled">
          <li class="wow bounceInLeft"><i class="fa fa-home"></i> Arturo Prat 548 Of 401, Antofagasta, Chile</li>
          <li class="wow bounceInLeft delay-1ms"><i class="fa fa-phone"></i> (55) 2-982909 </li>
          <li class="wow bounceInLeft delay-2ms"><i class="fa fa-envelope"></i> <a href="contacto@des-ing.cl">contacto@des-ing.cl</a></li>
          <li class="wow bounceInLeft delay-3ms"><i class="fa fa-globe"></i> <a href="http://des-ing.cl">www.des-ing.cl</a></li>
        </ul>
      </div>

      <div class="col-md-6">
        <%=	form_for :client, html: {id: 'sky-form3', class: 'sky-form contact-style' } do |m| %>
            <%= field_set_tag do %>
                <%= m.label :name, class: 'wow bounceInRight delay' do %>
                    Nombre<span class="color-red">*</span>
                <% end %>
                <div class="row">
                  <div class="col-md-7 margin-bottom-20 col-md-offset-0">
                    <div>
                      <%=	m.text_field :name, class: 'wow bounceInRight delay form-control' %>
                    </div>
                  </div>
                </div>

                <%=	 m.label :email, class: 'wow bounceInRight delay-1ms' do %>
                    Correo electónico<span class="color-red">*</span>
                <% end %>
                <div class="row">
                  <div class="col-md-7 margin-bottom-20 col-md-offset-0">
                    <div>
                      <%=	m.text_field :email, class: 'wow bounceInRight delay-1ms form-control' %>
                    </div>
                  </div>
                </div>

                <%=	 m.label :phone, class: 'wow bounceInRight delay-2ms' do %>
                    Número de contacto<span class="color-red">*</span>
                <% end %>
                <div class="row">
                  <div class="col-md-7 margin-bottom-20 col-md-offset-0">
                    <div>
                      <%=	m.text_field :phone, class: 'wow bounceInRight delay-2ms form-control' %>
                    </div>
                  </div>
                </div>

                <%=	 m.label :message, class: 'wow bounceInRight delay-3ms' do %>
                    Mensaje<span class="color-red">*</span>
                <% end %>
                <div class="row">
                  <div class="col-md-11 margin-bottom-20 col-md-offset-0">
                    <div>
                      <%=	m.text_area :message, rows: '8', class: 'wow bounceInRight delay-3ms form-control' %>
                    </div>
                  </div>
                </div>

                <%= m.button 'Enviar mensaje', 'data-style': 'zoom-in', 'data-size': 's', class: 'ladda-button btn-u btn-brd btn-brd-hover btn-u-dark wow bounceInRight delay-4ms' %>
            <% end %>
            <div class="message">
